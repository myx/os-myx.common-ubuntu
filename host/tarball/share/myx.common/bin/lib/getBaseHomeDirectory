#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

GetBaseHomeDirectory(){
	echo "/home"
}


case "$0" in
	*/myx.common/bin/lib/getBaseHomeDirectory) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common lib/getBaseHomeDirectory" >&2
			exit 1
		fi
		# set -e
		# GetBaseHomeDirectory "$@"
		echo "/home"
	;;
esac
